    "def is_leap(year):\n",
    "    leap = False\n",
    "    \n",
    "    if ((year % 4 == 0) & (year % 100 != 0)) | (year % 400 == 0):\n",
    "        leap = True\n",
    "    return leap\n",
    "\n",
    "year = int(input())\n",
    "print (is_leap(year))"
